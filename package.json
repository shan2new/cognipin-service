{
  "name": "cognipin-service",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "22.18.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "test:fetch-companies": "ts-node src/scripts/fetch-companies.ts --query vero",
    "lint": "eslint .",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts src/migrations/auto",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/data-source.ts",
    "backfill:logos": "ts-node src/scripts/backfill-logos-to-r2.ts",
    "backfill:logos:dry": "ts-node src/scripts/backfill-logos-to-r2.ts -- --dry-run",
    "fetch:roles": "ts-node src/scripts/fetch-software-engineer-roles.ts",
    "fetch:tech-roles": "ts-node src/scripts/fetch-tech-roles.ts",
    "roles:clean": "ts-node src/scripts/clean-tech-roles.ts",
    "roles:push": "ts-node src/scripts/push-roles.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@clerk/backend": "^1.9.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/typeorm": "^10.0.2",
    "@tavily/core": "^0.5.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "docx": "^9.5.1",
    "openai": "^5.12.2",
    "pg": "^8.12.0",
    "puppeteer": "^24.17.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.8.3"
  }
}
